#include<stdio.h>
#include<conio.h>
int main()
{
	int  number;
	printf("enter a number ( 1 to 5 )");
	scanf("%d",&number);
	switch(number)
	{
		case 1:
			printf("Food item-pizza \n");
			printf("price-Rs 239");
			break;
		case 2:
		     printf("Food item-Burger \n");
			 printf("price-Rs 129");
			 break;
		case 3:
			 printf("Food item-pasta \n");
			 printf("price-Rs 179");
			 break;
		case 4:
		     printf("Food item-French Fries\n");
			 printf("price-Rs 99");
			 break;
		case 5:
		     printf("Food item-sandwich\n");
			 printf("price-Rs-149");
			 break;
		default:
		     printf("out of range");
			 break;	 	 	 	
	}
	getch();
}

 
